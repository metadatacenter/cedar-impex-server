{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CEDARValidationResponse",
  "description": "Representation of the response returned by the CEDAR validator service",
  "type": "object",
  "properties": {
    "isValid": {
      "type": "boolean"
    },
    "messages": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "isValid",
    "messages"
  ]
}
